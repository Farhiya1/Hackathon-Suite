<div class="posts">
  {{#each projects}}
    <article class="card">
      <div class="card-header bg-light flex-row justify-space-between">
        <h2>
          {{!-- <a href="/post/{{id}}">{{title}}</a> --}}
        </h2>
        <div>
          Project starts on
          {{start_date}}
          until
          {{end_date}}
        </div>
      </div>
      <div class="card-body">
        {{body}}
        <button id="projectBtn"> Join Project</button>
      </div>
    </article>
  {{/each}}
</div>