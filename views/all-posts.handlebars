<div class="posts">
  {{#each projects}}
    <article class="card">
      <div class="card-header bg-light flex-row justify-space-between">
        <h2>
          <a href="/home-routes/{{id}}">{{title}}</a>
        </h2>
        <h5>Startup</h5><p>{{startup}}</p>
        <p>{{description}}</p>

        <div>
          Project starts on
          {{start_date}}
          until
          {{end_date}}
        </div>
      </div>
      <div class="card-body">
        {{body}}
        <button id="projectBtn"> Join Project</button>
      </div>
    </article>
  {{/each}}
</div>